<div class="row justify-content-center">
  <div class="col-6">
    <h2>Become a provider</h2>
    <%= simple_form_for([@user,@list]) do |f| %>
      <%= f.input :listing_type, collection: List::SERVICES %>
      <%= f.input :listing_title %>
      <%= f.input :location %>
      <%= f.input :price_rate %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
